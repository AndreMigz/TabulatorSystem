<div class="topbar pt-3">
  <div class="container-fluid">
    <h2 class="mt-3"> User Management</h2>
  </div>
</div>
<div>
  <div class="d-flex justify-content-end mr-0 mt-4 mb-3">
    <%= render partial: 'admin/members/add_member_modal', locals: { member: User.new } %>
  </div>
  <table class="table table-striped">
    <thead class="table-secondary">
      <tr>
        <th>Email</th>
        <th style="width: 10%;">Action</th>
      </tr>
    </thead>
    <tbody class="table-group-divider">
      <% @members.each do |member| %>
        <tr>
          <td><%= member.email %></td>
          <td>Edit Delete</td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
